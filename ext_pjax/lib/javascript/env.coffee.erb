if @Env?
  console.log "ExtPjax Overriding #{this.name}.Env"

class @Env
  @current: '<%= Rails.env.to_s %>'
  @debug_trace: <%= ExtPjax.config.debug_trace? %>

  @initialize: =>
    for env in ['development', 'test', 'vagrant', 'staging', 'production']
      @[env] = @is(env)

  @is: (env) =>
    @current == env

@Env.initialize()
