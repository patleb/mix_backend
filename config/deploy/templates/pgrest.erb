db-uri = "<%= Secret[:pgrest_uri] %>"
db-schema = "api"
db-anon-role = "web_anon"
db-pool = <%= Secret[:pgrest_pool] %>

server-host = "<%= Secret[:pgrest_server_host] %>"
server-port = <%= Secret[:pgrest_server_port] %>

<%= %{jwt-secret = "#{Secret[:pgrest_jwt_secret]}"} if Secret[:pgrest_jwt_secret].present? %>

max-rows = <%= Secret[:pgrest_max_rows] %>
