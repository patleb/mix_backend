db-uri = "<%= Setting[:pgrest_db_uri] %>"
db-schema = "api"
db-anon-role = "web_anon"
db-pool = <%= Setting[:pgrest_db_pool] %>

server-host = "<%= Setting[:pgrest_host] %>"
server-port = <%= Setting[:pgrest_port] %>

<%= %{jwt-secret = "#{Setting[:pgrest_jwt_secret]}"} if Setting[:pgrest_jwt_secret].present? %>

max-rows = <%= Setting[:pgrest_max_rows] %>
